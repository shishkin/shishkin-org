{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "types": ["astro/client"]
  },
  "include": ["src/**/*.*", "astro.config.ts"],
  "exclude": ["node_modules"]
}
